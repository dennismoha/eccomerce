<%- include('../../partials/admin/header'); %>
<%- include('../../partials/admin/sidenav'); %>

	<div class="jumbotron">
		<h3 style="text-align: center"> All Products</h3>
	</div>

 

  <table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">title</th>
      <th scope="col">Description</th>
      <th scope="col">price</th>
       <th scope="col">category</th>
    </tr>
  </thead>
  <tbody>
    <%products.forEach(function(product) { %>
    <tr>
      <th scope="row">1</th>
      <td><a href="/product/product/<%= product.id %>"> <%= product.title %></a></td>
      <td><a href="/product/product/<%= product.id %>"> <%= product.description %></a></td>
      <td><a href="/product/product/<%= product.id %>"> <%= product.price %></a></td>
       <td><a href="/product/product/<%= product.id %>"> <%= product.category %></a></td>
    </tr> 
          <% }) %>   
  </tbody>
</table>

		 
